      </ImageView>
      <Label layoutX="239.0" layoutY="29.0" text="Take Test" textFill="#ff8306">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Button fx:id="ocdBtn" layoutX="93.0" layoutY="111.0" mnemonicParsing="false" onAction="#ocdgo" prefHeight="102.0" prefWidth="229.0" style="-fx-background-radius: 50; -fx-background-color: #C96E50;" text="OCD" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="anxietyBtn" layoutX="355.0" layoutY="111.0" mnemonicParsing="false" onAction="#anxietygo" prefHeight="102.0" prefWidth="229.0" style="-fx-background-radius: 50; -fx-background-color: #C96E50;" text="Anxiety" textFill="WHITE">
