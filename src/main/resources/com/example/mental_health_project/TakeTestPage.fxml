            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="adhdBtn" layoutX="355.0" layoutY="240.0" mnemonicParsing="false" onAction="#adhdgo" prefHeight="102.0" prefWidth="229.0" style="-fx-background-radius: 50; -fx-background-color: #C96E50;" text="ADHD" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="home" layoutX="39.0" layoutY="35.0" mnemonicParsing="false" onAction="#home" style="-fx-background-color: #C96E50; -fx-background-radius: 7;" text="home" textFill="WHITE">
