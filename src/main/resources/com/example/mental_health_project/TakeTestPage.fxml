<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mental_health_project.TakeTestController">
   <children>
      <ImageView fitHeight="407.0" fitWidth="677.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
